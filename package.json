{
  "name": "craftcms-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Craft CMS webpack Boilerplate npm dependencies",
  "main": "main.js",
  "scripts": {
    "ds_store": "find . -name '.DS_Store' -type f -delete",
    "clean": "npm run ds_store && rimraf web/build",
    "start": "npm run clean && WEBPACK_CONFIG_TYPE='dev' webpack --progress --colors && WEBPACK_CONFIG_TYPE='watch' webpack-dev-server --hot --inline",
    "build": "npm run clean && WEBPACK_CONFIG_TYPE='build' webpack -p --progress --colors",
    "launch": "npm run clean && WEBPACK_CONFIG_TYPE='launch' webpack -p --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rppld/craftcms-webpack-boilerplate"
  },
  "author": "Rppld",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.2",
    "robotstxt-webpack-plugin": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "axios": "^0.16.2",
    "intersection-observer": "^0.4.2",
    "lozad": "^1.0.6",
    "picturefill": "^3.0.2",
    "vue": "^2.4.4"
  }
}
